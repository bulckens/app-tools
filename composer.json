{   
    "name": "bulckens/app-tools",
    "description": "Boilerplate tools for Slim + Eloquent + Twig based PHP apps",
    "type": "library",
    "license": "Proprietary",
    "authors": [
        {
            "name": "wout",
            "email": "wout@mick-wout.com"
        }
    ],
    "dist": {
        "url": "git@github.com:bulckens/api-tools.git",
        "type": "git"
    },
    "repositories": [
        {   "type": "vcs",
            "url": "git@github.com:bulckens/cli-tools.git",
            "no-api": true
        },
        {   "type": "vcs",
            "url": "git@github.com:bulckens/helpers.git",
            "no-api": true
        }
    ],
    "require": {
        "twig/twig": "^1.31",
        "twig/extensions": "^1.4",
        "monolog/monolog": "^1.21",
        "swiftmailer/swiftmailer": "^5.4",
        "illuminate/database": "^5.4",
        "symfony/yaml": "^3.2",
        "bulckens/cli-tools": "dev-master",
        "bulckens/helpers": "dev-master",
        "illuminate/support": "^5.4",
        "slim/slim": "^3.7",
        "cartalyst/sentinel": "^2.0",
        "illuminate/events": "^5.4",
        "symfony/http-foundation": "^3.2",
        "desarrolla2/cache": "^2.1",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2"
    },
    "autoload": {
        "psr-0": {
            "Bulckens": "src/"
        }
    }
}
